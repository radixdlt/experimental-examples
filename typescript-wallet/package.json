{
  "name": "@radixdlt/typescript-wallet",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "build": "rollup -c",
    "esm": "node --experimental-specifier-resolution=node --loader ts-node/esm",
    "account-address": "npm run esm ./src/scripts/account-address.ts",
    "generate-mnemonic": "npm run esm ./src/scripts/generate-mnemonic.ts",
    "faucet": "npm run esm ./src/scripts/faucet.ts",
    "gw:well-known": "npm run esm ./src/scripts/gw/well-known.ts",
    "gw:tx:lookup": "npm run esm ./src/scripts/gw/lookup-tx.ts",
    "gw:status": "npm run esm ./src/scripts/gw/status.ts",
    "gw:state:details": "npm run esm ./src/scripts/gw/state-details.ts",
    "examples:deploy-package": "npm run esm ./examples/deploy-package.ts"
  },
  "license": "SEE LICENSE",
  "dependencies": {
    "@radixdlt/radix-dapp-toolkit": "1.3.0",
    "@radixdlt/radix-engine-toolkit": "1.0.3",
    "bip39": "^3.1.0",
    "dotenv": "^16.3.1",
    "ed25519-hd-key": "^1.3.0",
    "hdkey": "^2.1.0",
    "lodash.chunk": "^4.2.0",
    "neverthrow": "^6.0.0",
    "rxjs": "^7.8.1",
    "tslog": "4.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@swc/core": "^1.3.70",
    "@types/hdkey": "^2.0.1",
    "@types/lodash.chunk": "^4.2.7",
    "@types/node": "^18.12.0",
    "nodemon": "^3.0.1",
    "rollup": "^4.1.5",
    "rollup-plugin-dts": "^6.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
